local Players = game:GetService("Players")

Players.PlayerAdded:Connect(function(player)
    while true do
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Niggers", "All")
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Faggot", "All")
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Gay Nigga Kys", "All")
    end
end)
